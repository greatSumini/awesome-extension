{
  "name": "Awesome.dev",
  "description": "Keep & Awesome your contents!",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Save this page"
  },
  "icons": {
    "16": "./icons/save192.png",
    "48": "./icons/save192.png",
    "128": "./icons/save192.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["activeTab"]
}
